<div class="project-pp">
    {% if resource.projects is not empty %}
    <ul class="col-lg-12 col-md-12 col-sm-12">
        {% for keyPrj, item in resource.projects %}
            <li class="col-lg-4 col-md-6 col-sm-12">
                <div class="li-container">
                    <div class="container-overflow">
                    <a href="{{ path('_slug', { 'url': item.slug }) }}" title="{{ item.title }}">
                        <div class="project-overlay"><h2>{{ item }}</h2></div>
                        {#{% for keyImg, image in item.images %}#}
                    {% if item.images.0.media.url is not empty %}
                        <div id="{{ keyPrj }}_img_0" class="img-preload">
                            {#<div class="loader">#}
                            {#<svg version="1.1" id="loader" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"#}
