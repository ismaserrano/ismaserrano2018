{% extends 'IsmaserranoPortfolioBundle:Page:layout.html.twig' %}
{% block content %}
    {{ render_pagetemplate(page) }}
        <div id="project-detail" class="col-lg-12 col-md-12 col-sm-12">
            {% if page.project %}
                <div id="detail-container">
                    <h1>{{ page.project.title }}</h1>
                    <div class="text-right">{{ page.project.body | raw }}</div>
                    {#{{ dump(page.project) }}#}
                </div>
            {% endif %}
        </div>
{% endblock %}
